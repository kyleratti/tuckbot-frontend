{
  "name": "tuckbot-frontend",
  "version": "1.0.0",
  "description": "The frontend for Tuckbot that serves video content to visitors",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm run start",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "npm run clean && webpack && echo tuckbot.tv > ./dist/CNAME",
    "deploy": "npm run build && gh-pages -d dist",
    "generate-thumbs": "ts-node-dev --cls tools/generate-thumbs.ts",
    "remove-exif": "ts-node-dev --cls tools/remove-exif.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyleratti/tuckbot-frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kyleratti/tuckbot-frontend/issues"
  },
  "homepage": "https://github.com/kyleratti/tuckbot-frontend#readme",
  "dependencies": {
    "dotenv-webpack": "^1.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-image-gallery": "^0.9.1",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@types/image-thumbnail": "^1.0.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-image-gallery": "^0.8.4",
    "@types/react-router-dom": "^5.1.3",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "file-loader": "^4.3.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^3.2.0",
    "image-thumbnail": "^1.0.6",
    "minimist": "^1.2.0",
    "piexifjs": "^1.0.6",
    "sass": "^1.24.4",
    "sass-loader": "^8.0.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
