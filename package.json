{
  "name": "tuckbot-frontend",
  "version": "1.0.0",
  "description": "The frontend for Tuckbot that serves video content to visitors",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "npm run start",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "prebuild": "npm run clean",
    "build": "webpack && echo tuckbot.tv > ./dist/CNAME",
    "predeploy": "npx react-snap",
    "deploy": "gh-pages -d dist",
    "generate-thumbs": "ts-node-dev --cls tools/generate-thumbs.ts",
    "remove-exif": "ts-node-dev --cls tools/remove-exif.ts",
    "serve": "npx http-server ./dist"
  },
  "reactSnap": {
    "source": "dist",
    "include": [
      "/index.html"
    ],
    "crawl": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyleratti/tuckbot-frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/kyleratti/tuckbot-frontend/issues"
  },
  "dependencies": {
    "dotenv-webpack": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@kyleratti/configurator": "^3.0.0",
    "@types/image-thumbnail": "^1.0.0",
    "@types/minimist": "^1.2.0",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@webpack-cli/serve": "^0.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "file-loader": "^6.0.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.3.0",
    "http-server": "^0.12.3",
    "image-thumbnail": "^1.0.10",
    "minimist": "^1.2.5",
    "piexifjs": "^1.0.6",
    "react-snap": "^1.23.0",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
