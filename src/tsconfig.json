{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "removeComments": true,
    "outDir": "../lib",
    "downlevelIteration": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "stripInternal": true,
    "strict": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "sourceMap": true
  },
  "exclude": []
}
